package Strings;

public class Reverseastring {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		String s = "i like programming";
		char g[] = s.toCharArray();
		for(int i = s.length();i>=0;i--) {
			System.out.print(g[i]);
		}
	}}
		
		
		
		
		

