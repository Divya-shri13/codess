package basicprgrms;

public class swap2numbers {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int a = 5;
int b = 10;
System.out.println("before swapp:");
System.out.println("a="+a);
System.out.println("b="+b);
int temp = a;
a=b;
b=temp;
System.out.println("after swapp:");
System.out.println("a="+a);
System.out.println("b="+b);
	}

}
